	<script>
		function switchTheme(id){
			loadingTheme = 'yes';
			el('picturebox_thumbnails').style.display = 'none';
			el('ajaxloader').style.display = 'block';
			httpGet("http://dominicweb.eu/wp-content/themes/traveler4/functions.php?theme="+id);
		}
		
		function sound(snd){
			player = initSound();
			playerwin = document.getElementById('playerwin');
			playerwin.style.display = "block";
			player.setAttribute('src', 'http://dominicweb.eu/media/?lang=eng&key='+snd);
		}
		function initSound(){
			player = document.getElementById('sndplayer');
			if(!player){
				player = document.createElement('audio');
				player.id = 'sndplayer';
				player.setAttribute('controls', 'true');
				player.setAttribute('autoplay', 'true');
				player.innerHTML = "Your browser does not support playing audio.";
				
				playerwin = document.createElement("div"); 
				playerwin.id = "playerwin";
				
				container = document.createElement("div"); 
				container.id = 'playercontainer';
								
				info = document.createElement("div");
				info.innerHTML = "The sound will be played automatically once completely loaded. <a href='javascript:hideSound();'>[Close]</a>";
				
				container.appendChild(player);
				container.appendChild(info);
				playerwin.appendChild(container);
				
				container.innerHTML = "<div style='display: table-cell; vertical-align: middle;'><div style='margin-left: auto; margin-right: auto;'>" + container.innerHTML + "</div></div>";
				
				document.getElementsByTagName("body")[0].appendChild(playerwin);
			}
			player.addEventListener('ended', function(){
				setTimeout(function(){hideSound();}, 1000);
			}, false);
						
			return player;
		}
		function hideSound(){
			document.getElementById('sndplayer').pause();
			playerwin = document.getElementById('playerwin');
			playerwin.style.display = "none";
		}
	</script>
	
